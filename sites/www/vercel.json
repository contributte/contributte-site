    { "src": "/partners", "status": 308, "headers": { "Location": "https://contributte.org/partners.html" } },
    { "src": "/donation", "status": 308, "headers": { "Location": "https://contributte.org/partners.html" } },
    { "src": "/donations", "status": 308, "headers": { "Location": "https://contributte.org/partners.html" } },
