    { "src": "/donates", "status": 308, "headers": { "Location": "https://contributte.org/partners.html" } },
    { "src": "/sponsor", "status": 308, "headers": { "Location": "https://contributte.org/partners.html" } },
    { "src": "/sponsors", "status": 308, "headers": { "Location": "https://contributte.org/partners.html" } },
    { "src": "/sponsorship", "status": 308, "headers": { "Location": "https://contributte.org/partners.html" } },
    { "src": "/vision.html", "status": 308, "headers": { "Location": "https://contributte.org/roadmap/vision.html" } },
